{
  "name": "rust-wasm-tutorial",
  "version": "1.0.0",
  "description": "Tutorial 4.5: Rust/WebAssembly Performance Enhancement",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "wasm:build": "cd src/rust && wasm-pack build --target web --out-dir ../wasm",
    "wasm:watch": "cargo watch -C src/rust -s 'wasm-pack build --target web --out-dir ../wasm'",
    "test": "cargo test -p genomic-wasm",
    "bench": "cargo bench -p genomic-wasm"
  },
  "dependencies": {
    "d3": "^7.8.5"
  },
  "devDependencies": {
    "vite": "^5.0.0",
    "vite-plugin-wasm": "^3.2.2",
    "vite-plugin-top-level-await": "^1.3.1"
  }
}
